# ===========================================
# Supabase Configuration (Required for Auth)
# ===========================================
# Get these values from: https://supabase.com/dashboard/project/xciveaaildyzbreltihu/settings/api

# Project URL
SUPABASE_URL=https://xciveaaildyzbreltihu.supabase.co

# Public anonymous key (safe for client-side, respects RLS)
SUPABASE_ANON_KEY=your-anon-key-here

# Service role key (server-side only, bypasses RLS - KEEP SECRET)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# JWT Secret for token verification
# Found in: Project Settings > API > JWT Settings
SUPABASE_JWT_SECRET=your-jwt-secret-here

# ===========================================
# Application Settings
# ===========================================
# Daily workflow call limit per user
DAILY_WORKFLOW_LIMIT=10

# ===========================================
# Existing DataFlow-Agent Settings
# ===========================================
# OpenAI-compatible API configuration
DF_API_KEY=your-api-key-here
DF_API_URL=https://api.openai.com/v1

# GPU pool for PDF parsing (comma-separated device IDs)
MINERU_DEVICES=0,1,2,3
